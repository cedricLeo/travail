{% block suggestionHotel %}
	<div class="suggestion ">
	  <h3>{{ "suggestion.hebergement" | trans }}</h3>
	  
	  {# Région 1 #}
		<div >
		   {% for r1 in displayregionLeft1 %}
		   	   {% if app.request.locale == "fr" %} 
			   <h2><a href="{{ path('_hebergementtyperegionfr',{'name':categorie.repertoire_fr|lower , 'region':r1.repertoirefr|lower}) }}" title="Hôtel{% if listClientGauche1|length > 1 %}s{% endif %} région {{ r1.nomfr }}">Hôtel{% if listClientGauche1|length > 1 %}s{% endif %} {{ r1.nomfr }}</a></h2>
			         <ul>
			         {% for qc in listClientGauche1 %}
			           <li><a href="{{ path('_hebergementinfoclient',{'name':qc.repertoirefr|lower}) }}" target="_blank" title="Hôtel {{ qc.nomfr }}">{{ qc.nomfr }}</a> {% if qc.prixapartir is not empty %} <span>{{ "a.partir" | trans }} {{ qc.prixapartir|replace({"$":""}) }}$</span>{% endif %}</li>
			         {% endfor %}
			         </ul>
			   <h4>[<a href="{{ path('_hebergementtyperegionfr',{'name':categorie.repertoire_fr|lower , 'region':r1.repertoirefr|lower}) }}" title="Hôtels région {{ r1.nomfr }}" >Voir les hôtels région {{ r1.nomfr }}</a>]</h4>
			   {% else %}
			   <h2><a href="{{ path('_hebergementtyperegionen',{'name':categorie.repertoire_en|lower , 'region':r1.repertoireen|lower}) }}" title="Hotel{% if listClientGauche1|length > 1 %}s{% endif %} region {{ r1.nomen }}">Hotel{% if listClientGauche1|length > 1 %}s{% endif %} {{ r1.nomen }}</a></h2>
			         <ul>
